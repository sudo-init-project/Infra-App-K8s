apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization

namespace: proyecto-cloud-staging

resources:
  - ../../base
  - nginx-configmap.yaml

namePrefix: staging-
nameSuffix: -stg

patches:
  - path: deployment_patch.yaml

images:
  - name: facundo676/backend-shop
    newTag: staging-v1
  - name: facundo676/frontend-shop
    newTag: staging-v1
